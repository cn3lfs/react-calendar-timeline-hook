    "jest-dom": "^3.5.0",
    "jest-watch-typeahead": "^1.1.0",
    "jsdom": "^19.0.0",
    "moment": "^2.29.3",
    "prettier": "^2.6.2",
    "prettier-eslint-cli": "^6.0.1",
    "prop-types": "^15.8.1",
    "randomcolor": "^0.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-testing-library": "^6.0.3",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "~3.3.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
